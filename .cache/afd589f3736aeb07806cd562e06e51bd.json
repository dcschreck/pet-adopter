":"Results.js","original":{"line":10,"column":28}},{"name":"Results","generated":{"line":110,"column":18},"source":"Results.js","original":{"line":73,"column":15}}],"sources":{"Results.js":"import React from \"react\";\nimport pf from \"petfinder-client\";\nimport Pet from \"./Pet\";\n\nconst petfinder = pf({\n  key: process.env.API_KEY,\n  secret: process.env.API_SECRET\n});\n\nclass Results extends React.Component {\n  constructor(props) {\n    //constructor calls the parent compontent's constructor (React.Component)\n    super(props);\n\n    //state are things that can change, as opposed to props, which are immutable; props always come from a parent into a child\n    this.state = {\n      pets: []\n    };\n  }\n  //lifecycle method; i've rendered do the DOM for the first time; gets called once per component\n  componentDidMount() {\n    petfinder.pet\n      .find({ output: \"full\", location: \"Seattle, WA\" })\n      .then(data => {\n        let pets;\n        //doing a lot of data checking because it's XML; if only one pet, Petfinder returns object. If more, returns object\n        if (data.petfinder.pets && data.petfinder.pets.pet) {\n          if (Array.isArray(data.petfinder.pets.pet)) {\n            pets = data.petfinder.pets.pet;\n          } else {\n            pets = [data.petfinder.pets.pet];\n          }\n        } else {\n          pets = [];\n        }\n\n        this.setState({\n          //this is the same as pets: pets\n          pets\n        });\n      });\n  }\n  render() {\n    return (\n      <div className=\"search\">\n        {this.state.pets.map(pet => {\n          //the map call above is transforming data objects into components\n          //the breed stuff is happening because petfinder sometimes has 2 breeds for an animal\n          let breed;\n\n          if (Array.isArray(pet.breeds.breed)) {\n            breed = pet.breeds.breed.join(\", \");\n          } else {\n            breed = pet.breeds.breed;\n          }\n          return (\n            <Pet\n              key={pet.id}\n              animal={pet.animal}\n              name={pet.name}\n              breed={breed}\n              media={pet.media}\n              location={`${pet.contact.city}, ${pet.contact.state}`}\n              id={pet.id}\n            />\n          );\n        })}\n      </div>\n    );\n  }\n}\n\nexport default Results;\n"},"lineCount":null}},"hash":"6b276c24b8cf05299680ab81c67c3374","cacheData":{"env":{"API_KEY":"808dd87df5fbaf984fcc5ff0dfbfabba","API_SECRET":"8d8c0e38a6a95bc8db8f76f57e2b6c84"}}}